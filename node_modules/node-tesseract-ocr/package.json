{
  "name": "node-tesseract-ocr",
  "version": "2.0.0",
  "description": "A Node.js wrapper for the Tesseract OCR API",
  "keywords": [
    "ocr",
    "tesseract",
    "text recognition",
    "optical character recognition",
    "image to text"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/zapolnoch/node-tesseract-ocr.git"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "test": "npm-run-all test:*",
    "test:lint": "eslint .",
    "test:unit": "tape test/unit.js",
    "test:e2e": "tape test/e2e.js",
    "test:audit": "npm audit --audit-level=high"
  },
  "devDependencies": {
    "eslint": "^7.1.0",
    "eslint-config-silence": "^0.7.1",
    "eslint-plugin-prettier": "^3.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rewire": "^5.0.0",
    "tape": "^5.0.1"
  }
